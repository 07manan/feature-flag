{
  "name": "featureflags-node-sdk",
  "version": "1.0.0",
  "description": "Node.js SDK for Feature Flag evaluation with local caching",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "feature-flags",
    "feature-toggle",
    "sdk",
    "evaluation"
  ],
  "author": "07manan",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
